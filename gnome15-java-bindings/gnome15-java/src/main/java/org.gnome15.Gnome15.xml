<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/org/gnome15/Service">
  <interface name="org.freedesktop.DBus.Introspectable">
    <method name="Introspect">
      <arg direction="out" type="s" />
    </method>
  </interface>
  <interface name="org.gnome15.Screen">
    <method name="SetReceiveActions">
      <arg direction="in"  type="b" name="enabled" />
    </method>
  </interface>
  <interface name="org.gnome15.Service">
    <method name="IsStopping">
      <arg direction="out" type="b" />
    </method>
    <method name="GetServerInformation">
      <arg direction="out" type="s" />
      <arg direction="out" type="s" />
      <arg direction="out" type="s" />
      <arg direction="out" type="s" />
    </method>
    <signal name="ScreenAdded">
      <arg type="v" name="screen_name" />
    </signal>
    <signal name="Started">
    </signal>
    <method name="Stop">
    </method>
    <signal name="ScreenRemoved">
      <arg type="v" name="screen_name" />
    </signal>
    <method name="IsStarted">
      <arg direction="out" type="b" />
    </method>
    <method name="GetDevices">
      <arg direction="out" type="as" />
    </method>
    <method name="GetScreens">
      <arg direction="out" type="as" />
    </method>
    <signal name="Stopped">
    </signal>
    <method name="IsStarting">
      <arg direction="out" type="b" />
    </method>
    <signal name="Stopping">
    </signal>
    <signal name="Starting">
    </signal>
  </interface>
</node>
